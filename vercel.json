{
    "version": 2,
    "buildCommand": "echo 'No build needed'",
    "builds": [
        {
            "src": "apps/api/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/apps/api/api/index.js"
        }
    ]
}